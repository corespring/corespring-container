{
  "player": {
    "src": ["<%= dist %>/js/render/**/*.js"],
    "dest": "<%= dist %>/js/prod-player.js"
  },
  "coreLibs": {
    "src": [
      "<%= dist %>/bower_components/es5-shim/es5-shim.min.js",
      "<%= dist %>/bower_components/console-polyfill/index.js",
      "<%= dist %>/bower_components/jquery/dist/jquery.min.js",
      "<%= dist %>/bower_components/jquery-ui/jquery-ui.min.js",
      "<%= dist %>/bower_components/angular/angular.min.js",
      "<%= dist %>/bower_components/lodash/dist/lodash.min.js",
      "<%= dist %>/bower_components/angular-ui-sortable/sortable.min.js",
      "<%= dist %>/bower_components/mathjs/dist/math.min.js",
      "<%= dist %>/bower_components/saxjs/lib/sax.js"
    ],
    "dest": "<%= dist %>/js/core-libs.js"
  },
  "core": {
    "dest": "<%= dist %>/js/prod-core.js",
    "src": [
      "<%= dist %>/bower_components/corespring-ng-components/build/corespring-ng-components.js",
      "<%= dist %>/js/corespring/core.js",
      "<%= dist %>/js/corespring/core-library.js",
      "<%= dist %>/js/corespring/server/init-core-library.js",
      "<%= dist %>/js/corespring/lodash-mixins.js",
      "<%= dist %>/js/common/**/*.js"
    ]
  },
  "catalog": {
    "dest": "<%= dist %>/js/prod-catalog.js",
    "src": [
      "<%= dist %>/js/corespring/core-library.js",
      "<%= dist %>/js/corespring/server/init-core-library.js",
      "<%= dist %>/js/catalog/**/*.js",
      "<%= dist %>/js/render/services/**/*.js",
      "<%= dist %>/js/render/directives/**/*.js",
      "<%= dist %>/js/render/controllers/**/*.js"
    ]
  },
  "catalogExtras": {
    "dest": "<%= dist %>/js/prod-catalog-extras.js",
    "src": [
      "<%= dist %>/bower_components/angular-route/angular-route.min.js",
      "<%= dist %>/bower_components/angular-ui-router/release/angular-ui-router.min.js"
    ]
  },
  "editor": {
    "dest": "<%= dist %>/js/prod-editor.js",
    "src": [
      "<%= dist %>/js/corespring/core-library.js",
      "<%= dist %>/js/corespring/server/init-core-library.js",
      "<%= dist %>/js/editor/**/*.js",
      "<%= dist %>/js/catalog/**/*.js",
      "<%= dist %>/js/render/services/**/*.js",
      "<%= dist %>/js/render/directives/**/*.js",
      "<%= dist %>/js/render/controllers/**/*.js"
    ]
  },
  "editorExtras": {
    "dest": "<%= dist %>/js/prod-editor-extras.js",
    "src": [
      "<%= dist %>/bower_components/angular-route/angular-route.min.js",
      "<%= dist %>/bower_components/angular-ui-router/release/angular-ui-router.min.js",
      "<%= dist %>/bower_components/select2/select2.js",
      "<%= dist %>/bower_components/angular-ui-select2/src/select2.js",
      "<%= dist %>/bower_components/angular-ui/build/angular-ui.min.js",
      "<%= dist %>/bower_components/angular-bootstrap/ui-bootstrap-tpls.js",
      "<%= dist %>/bower_components/bootstrap/js/dropdown.js",
      "<%= dist %>/bower_components/bootstrap/js/modal.js",
      "<%= dist %>/bower_components/bootstrap/js/tooltip.js",
      "<%= dist %>/bower_components/bootstrap/js/popover.js",
      "<%= dist %>/bower_components/angular-ui-ace/ui-ace.js",
      "<%= dist %>/bower_components/ace-builds/src-min-noconflict/ace.js",
      "<%= dist %>/bower_components/ace-builds/src-min-noconflict/theme-twilight.js",
      "<%= dist %>/bower_components/ace-builds/src-min-noconflict/mode-xml.js",
      "<%= dist %>/bower_components/ace-builds/src-min-noconflict/worker-json.js",
      "<%= dist %>/bower_components/ace-builds/src-min-noconflict/mode-json.js",
      "<%= dist %>/bower_components/jquery.browser/dist/jquery.browser.min.js",
      "<%= dist %>/bower_components/undo.js/undo.js"
    ]
  }
}